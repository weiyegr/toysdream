page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    font-size: 32rpx;
}

.header_info{
    height: 200rpx;
    width: 100%;
    background: #F6F6F6;
    border-bottom: 1px solid #e6e6e6;
    display: flex;
    flex-direction: row;
    font-size: 32rpx;
    margin-bottom: 20rpx;
    
}

.header_info_userinfo-avatar{
    height: 120rpx;
    width: 120rpx;
    border-radius:60rpx;
    margin: 30rpx 30rpx 30rpx 60rpx;
    float: left;
}
.header_info_userinfo-detail{
    height: 120rpx;
    width: 200rpx;
    margin: 30rpx;
    float: left;
}
.header_info_userinfo-detail-nickname{
  font-size: 36rpx;
  margin:10rpx 0 15rpx;
}

.list_detail{
    height: 100rpx;
    line-height: 100rpx;
    width: 100%;
    background: #F6F6F6;
    border-bottom: 1px solid #e6e6e6;
    font-size: 34rpx;
    position:relative;
}

.detail_icon{
    height: 50rpx;
    width: 50rpx;
    float:left;
    margin: 18rpx 15rpx 18rpx 18rpx;
}

.detail_title{
    height: 60rpx;
    line-height: 60rpx;
    width: 200rpx;
    float:left;
    font-size: 34rpx;
    margin: 20rpx 20rpx 20rpx 0;
}

.detail_icon_i{
    height: 100rpx;
    line-height: 100rpx;
    width: 100%;
    opacity:0;
    top:0rpx;
    left:0rpx;
    position:absolute;
    z-index: 100;
    border-bottom: 1px solid rgba(0, 0, 0, .1);
}

.login_out{
    height: 80rpx;
    width: 100%;
    background: #F6F6F6;
    border-bottom: 1px solid rgba(0, 0, 0, .1);
    margin-top: 30rpx;
    font-size: 32rpx;
    text-align: center;
    line-height: 80rpx;
}